{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "outputDirectory": "dist",
  "env": {
    "VITE_API_URL": "@vite-api-url"
  },
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.11"
    }
  },
  "redirects": [
    {
      "source": "/api/:path*",
      "destination": "https://kaleidoscope-backend.vercel.app/api/:path*"
    }
  ]
}
